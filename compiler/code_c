int fonction1(int * p){
    int b = *p;
    printf(b);
    *p = 5;
    return 2;
}

int main(){
    int a = 7;
    int * pointeur = &a;
    int c = fonction1(pointeur);
    if(c==2){ 
        printf(c);
        printf(*pointeur);
    }
    else{
        c=a+2;
    }
    while (c!=5){
        c = c+1;    
    }
    printf(c);
    return 0;
} 




